{
	"name": "ae-jsx-runner",
	"displayName": "AE *.jsx Runner",
	"description": "Run Adobe After Effects *.jsx Script without ExtendScript Toolkit.",
	"version": "0.1.0",
	"publisher": "Feng L.H.",
	"repository": {
		"type": "git",
		"url": "https://github.com/zpfz/vscode-ae-jsx-runner.git"
	},
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"ae",
		"after effects",
		"extendscript",
		"jsx",
		"runner"
	],
	"icon": "img/icon.png",
	"activationEvents": [
		"onCommand:run.JSXScript"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "run.JSXScript",
				"title": "运行 After Effects 脚本",
				"icon": {
					"light": "./img/light.svg",
					"dark": "./img/dark.svg"
				}
			}
		],
		"menus": {
			"editor/title": [{
				"when": "resourceExtname == .jsx",
				"command": "run.JSXScript",				
				"group": "navigation"
			}],
			"editor/context": [{
				"when": "resourceExtname == .jsx",
				"command": "run.JSXScript",				
				"group": "navigation"
			}],
			"editor/title/context": [{
				"when": "resourceExtname == .jsx",
				"command": "run.JSXScript",				
				"group": "navigation"
			}]
		}
	},
	"license": "MIT",
	"homepage": "https://github.com/zpfz/vscode-ae-jsx-runner/",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"@types/node": "^6.0.40"
	}
}